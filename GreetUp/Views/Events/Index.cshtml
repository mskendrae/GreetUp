@model IEnumerable<GreetUp.Models.Event>

@{
    ViewBag.Title = "Coding Events for October";
}

<div class="panel panel-default">
    <div class="panel-body">
        <h2 class="chgText">@ViewBag.Title</h2><br />
        <h3>
            @Html.ActionLink("Create New", "Create")
        </h3>
    </div>
</div>




@*  @Html.DisplayNameFor(model => model.EventDate)

    @Html.DisplayNameFor(model => model.Location)

    @Html.DisplayNameFor(model => model.description)

    @Html.DisplayNameFor(model => model.Going)-->  *@
<div class="row">

    @foreach (var item in Model)
    {

        <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
                <div class="caption">
                    <h3>@Html.DisplayFor(modelItem => item.Title)</h3>
                    <h4>@Html.DisplayFor(modelItem => item.EventDate) </h4>
                    <h4>@Html.DisplayFor(modelItem => item.Location)</h4>
                    <h5>@Html.DisplayFor(modelItem => item.Going) Attending </h5>
                    <p>@Html.DisplayFor(modelItem => item.description)</p>

                    <p><a href="https://www.meetup.com/Akron-Front-End-Development/events/234473904/" class="btn btn-primary" role="button">More Information</a></p>
                </div>
            </div>
        </div>
    }
</div>



